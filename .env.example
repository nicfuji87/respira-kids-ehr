# ===========================================
# CONFIGURAÇÃO DO SUPABASE
# ===========================================

# URL da instância do Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co

# Chave pública (anon key) do Supabase
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# ===========================================
# CONFIGURAÇÃO DE DESENVOLVIMENTO
# ===========================================

# Ambiente de execução (development, staging, production)
VITE_APP_ENV=development

# URL base da aplicação
VITE_APP_URL=http://localhost:5173

# ===========================================
# CONFIGURAÇÃO DE LOGS E MONITORAMENTO
# ===========================================

# Nível de logs (debug, info, warn, error)
VITE_LOG_LEVEL=debug

# Sentry DSN para monitoramento de erros (opcional)
VITE_SENTRY_DSN=

# ===========================================
# CONFIGURAÇÃO DE INTEGRAÇÕES
# ===========================================

# Chave da API do ViaCEP para busca de endereços
VITE_VIACEP_URL=https://viacep.com.br/ws

# URL da API de webhooks (opcional)
VITE_WEBHOOK_URL=

# ===========================================
# CONFIGURAÇÃO DE SEGURANÇA
# ===========================================

# AI dev note: VITE_WEBHOOK_SECRET foi removido por questões de segurança
# Secrets não devem ser expostos no frontend. Use Edge Functions do Supabase 
# para validar webhooks no backend.

# Timeout para requisições HTTP (em ms)
VITE_HTTP_TIMEOUT=30000

# ===========================================
# CONFIGURAÇÃO MÉDICA
# ===========================================

# URL da API do CFM para validação de CRM (opcional)
VITE_CFM_API_URL=

# URL da API do DataSUS (opcional)
VITE_DATASUS_URL= 